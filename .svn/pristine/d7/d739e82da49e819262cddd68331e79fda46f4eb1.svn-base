create database rrphDB;

use rrphDB;

create table players(
pid int(32) not null auto_increment primary key,
name varchar(32),
rids varchar(1024),
) charset=gb2312;

create table roomRecord(
rid int(32) not null auto_increment primary key,
js char(1),
zz char(1),
playids varchar(1024),
) charset=gb2312;

create table playRecord(
playid int(32) not null auto_increment primary key,
p1name varchar(32),
p1score int(32),
p2name varchar(32),
p2score int(32),
p3name varchar(32),
p3score int(32),
p4name varchar(32),
p4score int(32)
) charset=gb2312;